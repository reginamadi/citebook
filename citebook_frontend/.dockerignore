# Ignore node_modules to avoid copying installed dependencies
node_modules
npm-debug.log
yarn-error.log

# Ignore logs
*.log
*.log.*

# Ignore temporary files
*.tmp
*.temp

# Ignore .env files (unless you specifically want to copy it into your Docker container)
.env
.env.local
.env.development
.env.production
.env.test

# Ignore build output (depending on your project setup)
dist/
build/

# Ignore git and other version control directories and files
.git
.gitignore
.gitattributes

# Ignore Docker-related files (optional)
.dockerignore
Dockerfile

# Ignore IDE/editor config and cache
.vscode/
.idea/
*.swp
*.swo
*.swn

# Ignore coverage folder (for testing results)
coverage/

# Ignore anything else specific to your project
# For example, compiled JS files or other temporary files
*.out
*.log.*
.cache/
